{
  "manifest_version": 2,
  "name": "Jibbermaster's Format Selector",
  "short_name": "MTG Formats",
  "start_url": "./formats",
  "description": "Random game format generator for groups of Magic the Gathering players.",
  "lang": "en-US",
  "default_locale": "en",
  "display": "fullscreen",
  "orientation": "any",
  "theme_color": "#000000",
  "background_color": "#007bff",
  "icons": [
    {
      "src": "./mtg/icon/evg_180.png",
      "type": "image/png",
      "sizes": "180x180"
    },
    {
      "src": "./mtg/icon/evg_192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "./mtg/icon/evg_512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ]
}
