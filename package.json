{
  "name": "magic.jibbermaster.com",
  "version": "0.1.0",
  "private": true,
  "description": "A suite of tools for Magic the Gathering",
  "keywords": [
    "magic the gathering",
    "mtg",
    "magic"
  ],
  "homepage": "https://magic.jibbermaster.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egreer/magic.jibbermaster.com.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://www.paypal.me/egreerme"
    }
  ],
  "type": "module",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html analysis.html",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky",
    "prettier": "prettier --cache --write '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "prettier:nocache": "prettier --write '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "pretty-quick": "pretty-quick",
    "pretty-quick:staged": "pretty-quick --staged --pattern '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "serve": "vite preview",
    "start": "vite",
    "update-packages": "npx npm-check-updates -u"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "case": "^1.6.3",
    "classnames": "^2.5.1",
    "cytoscape": "^3.30.4",
    "jquery": "^3.7.1",
    "keyrune": "^3.16.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "numeral-prefix": "^1.0.0",
    "pluralize": "^8.0.0",
    "popper.js": "^1.16.1",
    "rc-slider": "^11.1.8",
    "rc-tooltip": "^6.3.2",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.7",
    "react-cytoscapejs": "^2.0.0",
    "react-dice-complete": "^2.2.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1",
    "react-textfit": "^1.1.1",
    "recharts": "^2.15.0",
    "store": "^2.0.12",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/store": "^2.0.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "pretty-quick": "^4.0.0",
    "sass": "^1.83.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-pwa": "^0.21.1"
  },
  "overrides": {
    "react-textfit": {
      "react": ">=16.0.0",
      "react-dom": ">=16.0.0"
    },
    "eslint-plugin-react-hooks": {
      "eslint": "^9.8.0"
    }
  }
}
