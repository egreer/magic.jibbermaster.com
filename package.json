{
  "name": "magic.jibbermaster.com",
  "version": "0.1.0",
  "private": true,
  "description": "A suite of tools for Magic the Gathering",
  "keywords": [
    "magic the gathering",
    "mtg",
    "magic"
  ],
  "homepage": "https://magic.jibbermaster.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egreer/magic.jibbermaster.com.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://www.paypal.me/egreerme"
    }
  ],
  "type": "module",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html analysis.html",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky",
    "prettier": "prettier --cache --write '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "prettier:check": "prettier --cache --check '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "prettier:nocache": "prettier --write '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "pretty-quick": "pretty-quick",
    "pretty-quick:staged": "pretty-quick --staged --pattern '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "serve": "vite preview",
    "start": "vite",
    "update:check": "npx npm-check-updates",
    "update:all": "npx npm-check-updates -u"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "axios": "^1.13.6",
    "bootstrap": "^5.3.8",
    "case": "^1.6.3",
    "classnames": "^2.5.1",
    "cytoscape": "^3.33.1",
    "jquery": "^4.0.0",
    "keyrune": "^3.18.0",
    "lodash": "^4.17.23",
    "moment": "^2.30.1",
    "numeral-prefix": "^1.0.0",
    "pluralize": "^8.0.0",
    "popper.js": "^1.16.1",
    "rc-slider": "^11.1.9",
    "rc-tooltip": "^6.4.0",
    "react": "^19.2.4",
    "react-bootstrap": "^2.10.10",
    "react-cytoscapejs": "^2.0.0",
    "react-dice-complete": "^2.2.0",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.1",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.13.1",
    "react-router-dom": "^7.13.1",
    "react-textfit": "^1.1.1",
    "recharts": "^3.7.0",
    "store": "^2.0.12",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.23",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/store": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "pretty-quick": "^4.2.2",
    "sass": "^1.97.3",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0"
  },
  "overrides": {
    "react-dice-complete": {
      "react": ">=16.0.0",
      "react-dom": ">=16.0.0"
    },
    "react-helmet-async": {
      "react": ">=16.0.0",
      "react-dom": ">=16.0.0"
    },
    "react-textfit": {
      "react": ">=16.0.0",
      "react-dom": ">=16.0.0"
    },
    "eslint-plugin-react-hooks": {
      "eslint": "^9.8.0"
    }
  }
}
