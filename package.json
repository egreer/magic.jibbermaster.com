{
  "name": "magic.jibbermaster.com",
  "version": "0.1.0",
  "private": true,
  "description": "A suite of tools for Magic the Gathering",
  "keywords": [
    "magic the gathering",
    "mtg",
    "magic"
  ],
  "homepage": "https://magic.jibbermaster.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egreer/magic.jibbermaster.com.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://www.paypal.me/egreerme"
    }
  ],
  "type": "module",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html analysis.html",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky",
    "prettier": "prettier --cache --write 'src/**/*.{ts,js,tsx,jsx,css,scss}'",
    "prettier:nocache": "prettier --write 'src/**/*.{ts,js,tsx,jsx,css,scss}'",
    "pretty-quick": "pretty-quick",
    "pretty-quick:staged": "pretty-quick --staged --pattern 'src/**/*.{ts,js,tsx,jsx,css,scss}'",
    "serve": "vite preview",
    "start": "vite",
    "update-packages": "npx npm-check-updates -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "axios": "^1.7.1",
    "bootstrap": "^5.3.3",
    "case": "^1.6.3",
    "classnames": "^2.5.1",
    "cytoscape": "^3.29.2",
    "jquery": "^3.7.1",
    "keyrune": "^3.14.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "numeral-prefix": "^1.0.0",
    "pluralize": "^8.0.0",
    "popper.js": "^1.16.1",
    "rc-slider": "^10.6.2",
    "rc-tooltip": "^6.2.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.2",
    "react-cytoscapejs": "^2.0.0",
    "react-dice-complete": "^2.2.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "react-textfit": "^1.1.1",
    "recharts": "^2.12.7",
    "store": "^2.0.12",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/store": "^2.0.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "pretty-quick": "^4.0.0",
    "sass": "^1.77.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^0.20.0"
  },
  "overrides": {
    "react-textfit": {
      "react": ">=16.0.0",
      "react-dom": ">=16.0.0"
    }
  }
}
