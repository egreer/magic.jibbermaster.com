    },
    {
      "src": "./mtg/icon/starter_128.png",
      "type": "image/png",
      "sizes": "128x128"
    },
    {
      "src": "./mtg/icon/starter_256.png",
      "type": "image/png",
      "sizes": "256x256"
    }
  ]
}
